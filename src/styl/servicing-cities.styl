/* Servicing  Cities */

.servicing-cities
  margin-top 50px

  h4
    text-align center
    margin-bottom 20px

  div
    display flex
    justify-content center

    ul
      list-style none
      max-width 142px

      li
        font-size 16px
        color #555555
        line-height 22px

  h6
    text-align center
    font-weight normal
    margin-top 20px

    a
      color #0079D9
      font-weight bold
